max-visible=5
sort=-time
output=""
layer=top
anchor=top-right
font=JetBrainsMono NF 10
background-color=#09030a
text-color=#85a1bc
width=300
height=200
margin=10
padding=10
border-size=1
border-color=#481546
border-radius=2
progress-color=over #481546
icons=1
max-icon-size=64
icon-path=$HOME/.local/share/icons/flattrcolor-dark
markup=1
default-timeout=2400
actions=1
format=%s
ignore-timeout=0
group-by=none
invisible=0
on-notify=exec mpv /usr/share/sounds/freedesktop/stereo/message.oga

[mode=do-not-disturb]
invisible=1
on-notify=none
